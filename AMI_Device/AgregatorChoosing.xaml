<Window x:Class="AMI_Device.AgregatorChoosing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AMI_Device"
        mc:Ignorable="d"
        Title="AgregatorChoosing" Height="300" Width="300">
    <Grid>
        <ComboBox x:Name="IDcmbBox" HorizontalAlignment="Left" Margin="54,33,0,0" VerticalAlignment="Top" Width="169" Height="31" Loaded="comboBox_Loaded" SelectionChanged="comboBox_SelectionChanged"/>
        <Button x:Name="okBtn" Content="OK" HorizontalAlignment="Left" Margin="54,204,0,0" VerticalAlignment="Top" Width="169" Height="44" Click="okBtn_Click"/>

    </Grid>
</Window>
